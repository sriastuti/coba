/* tab for registration form ---------------------------------------------------------------------------- */
.my-tab{
 color:#fff;
}
.my-tab > li > a{
  color:#fff;
}
.my-tab > li {
  background-color: #c4e8b6;
  color:#fff;
}
.my-tab > li.active, .my-tab > li.active > a, .my-tab > li.active > a:hover, .my-tab > li.active > a:focus {
  background-color: #7DBE64;
  color:#fff;
}
.my-tab > li > a:hover{
    background-color: #7DBE64;
    
    border-radius: 0;
    color:#fff;
}
