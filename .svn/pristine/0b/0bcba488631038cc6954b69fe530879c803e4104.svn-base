/* sidebar menu------------------------------------------------------------------------------------ */
.sidebar-menu>li.header{color:#edeed6 !important;background:#7DBE64 !important}
.main-sidebar{background-color:#fc6917 !important}
.sidebar-menu>li:hover>a,.skin-blue .sidebar-menu>li.active>a{color:#b8c7ce !important;background:#de5a10 !important;border-left-color:#91bd7f !important}
.skin-blue .sidebar a{color:#fff !important}
.skin-blue .sidebar-menu > li > .treeview-menu {
    background: #fb8b4e ;
}
.skin-blue .treeview-menu > li > a {
    color: #af4104 !important;
}
.skin-blue .treeview-menu > li:hover > a {
